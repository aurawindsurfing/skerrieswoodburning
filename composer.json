{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "64robots/nova-fields": "^0.6.7",
        "barryvdh/laravel-dompdf": "^0.8.5",
        "consoletvs/invoices": "^1.1",
        "fideloper/proxy": "^4.0",
        "gregoriohc/laravel-nova-theme-responsive": "^0.6.1",
        "grimzy/laravel-mysql-spatial": "^2.1",
        "guzzlehttp/guzzle": "^6.3",
        "inspheric/nova-email-field": "^1.3",
        "inspheric/nova-indicator-field": "^1.2",
        "inspheric/nova-url-field": "^1.1",
        "laraning/nova-time-field": "^0.1.0",
        "laravel/framework": "^6.13",
        "laravel/nova": "^2.3",
        "laravel/tinker": "^2.0",
        "llaski/nova-scheduled-jobs": "^1.6",
        "maatwebsite/laravel-nova-excel": "^1.1",
        "mpdf/mpdf": "^7.1",
        "nexmo/client": "^1.5",
        "nexmo/laravel": "^1.1",
        "orlyapps/nova-belongsto-depend": "^0.0.6",
        "outhebox/nova-hidden-field": "^1.0",
        "peterbrinck/nova-laravel-news": "^0.4.0",
        "phpoffice/phpspreadsheet": "^1.5",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.1",
        "richardkeep/nova-timenow": "^0.0.5",
        "silvanite/nova-field-cloudinary": "^1.0",
        "spatie/laravel-activitylog": "^3.8",
        "titasgailius/search-relations": "^1.0",
        "understand/understand-laravel5": "^2.0",
        "vyuldashev/nova-money-field": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "facade/ignition": "^1.4"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}